my_list = [1, 8, 5, 8, 7, 9]
sorted(my_list)
while True:
    a = input("Введите натуральное число: ")
    while True:
        if a.isdigit():
            a = int(a)
            break
        else:
            a = input("Введите натуральное число: ")
    my_list.append(a)
    my_list = sorted(my_list)
    print(my_list)
